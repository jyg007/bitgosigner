services:
  bitgosigner:
    image: IMAGE
    ports:
      - "4433:4433"
    privileged: true 
    environment:
      CA_CRT: ${BITGO_CA_CRT}
      SERVER_CRT: ${BITGO_SERVER_CRT}
      SERVER_KEY: ${BITGO_SERVER_KEY}
      DB_PATH: ${BITGO_DB_PATH}
      HSM: ${BITGO_HSM}
    volumes:
      - "/mnt/data:/data:Z"
